{
  "name": "dcmjs-dimse",
  "version": "0.0.1",
  "description": "DICOM DIMSE implementation for NodeJS using dcmjs",
  "main": "dcmjs-dimse.min.js",
  "module": "dcmjs-dimse.min.js",
  "scripts": {
    "clean": "npm run clean:build && npm run clean:examples",
    "clean:build": "shx rm -rf build",
    "build": "npm run lint && npm run test && npm run webpack",
    "start": "npm run webpack",
    "webpack": "webpack --progress --config webpack.config.js",
    "build:examples": "npm run build && shx cp -r build/* examples",
    "clean:examples": "shx rm examples/dcmjs-dimse.min.js",
    "dev:examples": "nodemon examples/index.js",
    "start:examples": "node examples/index.js",
    "test": "mocha 'test/**/*.test.js'",
    "lint": "eslint -c .eslintrc.json --fix src && prettier --tab-width 2 --single-quote true --print-width 200 --use-tabs false --write src/**/*.js test/**/*.js examples/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PantelisGeorgiadis/dcmjs-dimse.git"
  },
  "keywords": [
    "DICOM",
    "DIMSE",
    "dcmjs"
  ],
  "author": "Pantelis Georgiadis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PantelisGeorgiadis/dcmjs-dimse/issues"
  },
  "homepage": "https://github.com/PantelisGeorgiadis/dcmjs-dimse#readme",
  "dependencies": {
    "buffer-chunks": "^0.1.1",
    "dcmjs": "^0.18.3",
    "smart-buffer": "^4.1.0",
    "ts-mixer": "^5.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "prettier": "^1.13.7",
    "shx": "^0.3.3",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
